{
  "builds": [
    {
      "src": "vercel-build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  
    "redirects": [
      { "source": "/", "destination": "https://docs.google.com/forms/d/1gjUFOvNS-35djtsNUn-eFFHGktdzKNtntFRjhZgpH2Y/", "permanent": true },
      { "source": "/form", "destination": "https://docs.google.com/forms/d/1gjUFOvNS-35djtsNUn-eFFHGktdzKNtntFRjhZgpH2Y/", "permanent": true}
    ],

    "rewrites" : [
      {
        "source": "/:path*",
        "destination": "/form"
      }
    ]
  
}
